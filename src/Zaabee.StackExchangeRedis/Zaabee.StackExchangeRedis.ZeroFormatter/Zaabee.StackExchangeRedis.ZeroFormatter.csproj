    <ItemGroup>
      <PackageReference Include="Zaabee.StackExchangeRedis.Serializer.Abstractions" Version="2021.1.0" />
      <PackageReference Include="Zaabee.ZeroFormatter" Version="2021.1.3" />
    </ItemGroup>

</Project>
