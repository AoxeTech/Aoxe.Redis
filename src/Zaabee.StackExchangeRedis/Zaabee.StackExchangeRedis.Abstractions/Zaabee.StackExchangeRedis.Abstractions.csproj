        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2022.4.0</PackageVersion>
        <Version>2022.4.0</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
