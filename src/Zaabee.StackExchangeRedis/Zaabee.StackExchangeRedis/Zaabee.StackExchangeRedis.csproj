<Project Sdk="Microsoft.NET.Sdk">
	
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;net6.0;net7.0;net8.0</TargetFrameworks>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageVersion>2023.2.1</PackageVersion>
		<Version>2023.2.1</Version>
		<Authors>Mutuduxf</Authors>
		<Company>Mutuduxf</Company>
		<PackageTags>Zaabee;Redis;StackExchange.Redis</PackageTags>
		<Description>Redis wrappers by StackExchange.Redis</Description>
		<PackageProjectUrl>https://github.com/PicoHex/Zaabee.Redis</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
	</PropertyGroup>
	
	<ItemGroup>
		<PackageReference Include="StackExchange.Redis" Version="2.7.17" />
		<PackageReference Include="Zaabee.Serializer.Abstractions" Version="2024.1.0" />
	</ItemGroup>
	
	<ItemGroup>
	  <ProjectReference Include="..\Zaabee.StackExchangeRedis.Abstractions\Zaabee.StackExchangeRedis.Abstractions.csproj" />
	</ItemGroup>
	
</Project>