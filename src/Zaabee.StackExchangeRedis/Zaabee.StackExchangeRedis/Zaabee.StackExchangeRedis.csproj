<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;netcoreapp3.1</TargetFrameworks>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageVersion>2022.4.8.8</PackageVersion>
		<Version>2022.4.8.8</Version>
		<Authors>Mutuduxf</Authors>
		<Company>Mutuduxf</Company>
		<PackageTags>Zaabee;Redis;StackExchange.Redis</PackageTags>
		<Description>Redis wrappers by StackExchange.Redis</Description>
		<PackageProjectUrl>https://github.com/PicoHex/Zaabee.Redis</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="StackExchange.Redis" Version="2.6.80" />
		<PackageReference Include="Zaabee.Serializer.Abstractions" Version="2022.14.0" />
		<PackageReference Include="Zaabee.StackExchangeRedis.Abstractions" Version="2022.2.0" />
	</ItemGroup>
</Project>