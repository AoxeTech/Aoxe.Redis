<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;netcoreapp3.1;net5.0;net6.0</TargetFrameworks>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>2022.2.0</PackageVersion>
        <Version>2022.2.0</Version>
        <Authors>Mutuduxf</Authors>
        <Company>Mutuduxf</Company>
        <PackageTags>Zaabee;Redis;StackExchange.Redis</PackageTags>
        <Description>Redis wrappers by StackExchange.Redis</Description>
        <PackageProjectUrl>https://github.com/PicoHex/Zaabee.Redis</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <LangVersion>latest</LangVersion>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="StackExchange.Redis" Version="2.2.88" />
      <PackageReference Include="Zaabee.Serializer.Abstractions" Version="2022.4.0" />
      <PackageReference Include="Zaabee.StackExchangeRedis.Abstractions" Version="2022.2.0" />
    </ItemGroup>

</Project>
