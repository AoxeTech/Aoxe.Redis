    <ItemGroup>
      <PackageReference Include="Zaabee.StackExchangeRedis.ISerialize" Version="1.0.0" />
      <PackageReference Include="Zaabee.ZeroFormatter" Version="1.0.1" />
    </ItemGroup>

</Project>
